{
  "buildCommand": "npm run build",
  "outputDirectory": "../dist",
  "framework": "vite",
  "rewrites": [
    { "source": "./api/(.*)", "destination": "/api/$1" },
    { "source": "./(.*)", "destination": "/index.html" },
    {
      "source": "/api/contact",
      "destination": "http://0.0.0.0:3000/api/contact"
    }
  ]
}
